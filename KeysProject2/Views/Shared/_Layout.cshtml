<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Keys 2 - Onboarding</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
@Html.Partial("_Menu")

<div class="container body-content">
    @RenderBody()
    <hr/>
    <footer>
        <p>&copy; @DateTime.Now.Year - Keys 2 - Onboarding</p>
    </footer>
</div>
    

@Scripts.Render("~/bundles/DataBundle")
@RenderSection("scripts", required: false)
</body>
</html>



